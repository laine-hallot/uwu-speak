{
  "name": "uwu-script",
  "displayName": "uwu-script",
  "description": "Syntax highlighting for UwU script",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "build": "node index.js"
  },
  "contributes": {
    "languages": [
      {
        "id": "uwu-script",
        "aliases": [
          "UwUScript",
          "uwu-script"
        ],
        "extensions": [
          ".uwu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "uwu-script",
        "scopeName": "source.uwu",
        "path": "./syntaxes/uwu-script.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.3.1"
  },
  "dependencies": {
    "tm-grammars": "^1.23.7"
  }
}
